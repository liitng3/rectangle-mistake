{
    "env": {
      "browser": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "VariableDeclarator": {"var": 2, "let": 2, "const": 3}
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": [
        "error",
        { "allow": ["error"] }
      ]
    }
  }